{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsxImportSource": "nativewind",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "metro.config.js"
, "app/(auth)/_layout.jsx", "app/index.jsx", "app/_layout.jsx", "app/(tabs)/profile.jsx"  ]
}
